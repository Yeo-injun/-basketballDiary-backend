<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.threeNerds.basketballDiary.mvc.repository.MemberRepository">

    <select id="saveMember" resultType="com.threeNerds.basketballDiary.mvc.domain.Member">
        INSERT INTO MEMBER (MEMBERID,PASSWORD,CITY,STREET,ZIPCODE,PHONENUMBER)
        VALUES (#{memberID},#{password},#{city},#{street},#{zipCode},#{phoneNumber})
    </select>

</mapper>