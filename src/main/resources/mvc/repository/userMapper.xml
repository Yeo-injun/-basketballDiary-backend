<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.threeNerds.basketballDiary.mvc.repository.UserRepository">

    <insert id="saveUser">
        INSERT INTO USER(USER_ID,PWD,EMAIL,GENDER,HEIGHT,WEIGHT,REG_DATE,UPDATE_DATE,USER_REG_YN,CITY,SIDO_COD,SGG_COD)
        VALUES(#{userid},#{password},#{email},#{gender},#{hegith},#{weight},#{regDate},#{updateDate},#{userRegYn},#{city},#{sidoCod},#{sggCod})
    </insert>

</mapper>