<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.threeNerds.basketballDiary.mvc.repository.UserRepository">

    <insert id="saveTeam">
        INSERT INTO TEAM(LEADER_ID, TEAM_NAME,HOMETOWN,INTRODUCTION,FOUNDATION_YMD,REG_DATE,UPDATE_DATE,SIDO_CODE,SIGUNGU_CODE)
        VALUES (#{leaderId},#{teamName},#{hometown},#{introduction},#{foundationYmd},#{regDate},#{updateDate},#{sidoCode},#{sigunguCode})
    </insert>
</mapper>